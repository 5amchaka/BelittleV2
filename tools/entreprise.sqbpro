alue="300"/><column index="3" value="151"/><column index="4" value="300"/><column index="5" value="49"/><column index="6" value="39"/><column index="7" value="61"/><column index="8" value="300"/><column index="9" value="108"/><column index="10" value="300"/><column index="11" value="300"/><column index="12" value="107"/><column index="13" value="118"/><column index="14" value="300"/></column_widths><filter_values><column index="1" value="sab"/></filter_values><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">SELECT 
    e.nom_entreprise, 
    e.adresse || ', ' || 
    COALESCE(cp.code_postal || ' ', '') || 
    COALESCE(v.nom_ville, '') || 
    CASE 
        WHEN e.id_cedex IS NOT NULL THEN ' ' || c.nom_cedex
        ELSE ''
    END AS adresse_complete
FROM entreprise e
LEFT JOIN villes v ON e.id_ville = v.id_ville
LEFT JOIN code_postal cp ON e.id_cp = cp.id_cp
LEFT JOIN cedex c ON e.id_cedex = c.id_cedex
ORDER BY e.nom_entreprise;
</sql><current_tab id="0"/></tab_sql></sqlb_project>
